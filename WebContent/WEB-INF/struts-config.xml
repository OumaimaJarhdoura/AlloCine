<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE struts-config PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans type="org.apache.struts.action.ActionFormBean">
		<form-bean name="ResearchContactForm" type="actionform.ResearchContactForm" />
		<form-bean name="UpdateContactForm" type="actionform.UpdateContactForm" />
		<form-bean name="CreateContactForm" type="actionform.CreateContactForm" />
		<form-bean name="DeleteContactForm" type="actionform.DeleteContactForm" />
		<form-bean name="UpdateContactResearchForm" type="actionform.UpdateContactResearchForm" />
	</form-beans>

	<action-mappings>
		<action path="/Login" type="action.LoginAction" name="LoginForm"
			scope="request" input="/index.jsp">
			<forward name="success" path="/main.jsp" />
			<forward name="error" path="/index.jsp" />
		</action>

		
		<action path="/CreateMovie" type="action.CreateMovieAction"
			name="CreateMovieForm" scope="request" input="/CreateMovie.jsp">
			<forward name="success" path="/main.jsp" />
			<forward name="error" path="/CreateMovie.jsp" />
		</action>
		
		
	</action-mappings>
	<!-- ========== message file =========================== -->
	<message-resources parameter="resources" />
</struts-config>